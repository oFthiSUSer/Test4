local Rooms:Folder = workspace:WaitForChild("GeneratedRooms")
local Remote:RemoteEvent = game:GetService("ReplicatedStorage"):WaitForChild("SpawnEntity")
local rng = Random.new()

local EntitiesList = [[A-5
